{
  "name": "dyrynda/maxo-php",
  "description": "A PHP SDK for the Maxo Telecommunications SMS API",
  "type": "library",
  "require": {
    "saloonphp/saloon": "^3.8"
  },
  "require-dev": {
    "pestphp/pest": "^2.34",
    "laravel/pint": "^1.15"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Dyrynda\\Maxo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Michael Dyrynda",
      "email": "michael@dyrynda.com.au"
    }
  ],
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "fix-style": "vendor/bin/pint",
    "check-style": "vendor/bin/pint --dry-run",
    "test": "vendor/bin/pest"
  }
}
